	    }
	    public float[] getSample() {;
	        return sample;
	    }

	    public float getSample(int i) {
	        float[] sample = getSample();
	        int i2 = this.shifted + i;
	        if (i2 < 0 || i2 >= sample.length) {
	            return 0.0f;
	        }
	        return sample[i2];
	    }
	    public byte[] getData() {
	        return this.data;
	    }

	   

	
